{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "$"
  },
  "rules": {
    "indent": ["error", 2, {"VariableDeclarator": 1, "SwitchCase": 1}],
    "max-len": ["error", 120, 2],
    "object-curly-spacing": ["error", "never"],
    "quotes": ["error", "single", {"allowTemplateLiterals": true}],
    "import/extensions": 0,
    "import/no-unresolved": 0
  }
}
